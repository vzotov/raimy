# OpenAI API Key
# Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Google OAuth Configuration
# Get these from https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Service API Key for internal service authentication
# Generate a secure random string (e.g., using openssl rand -hex 32)
SERVICE_API_KEY=

# PostgreSQL Database Configuration
# These are used for local development with Docker
POSTGRES_DB=raimy
POSTGRES_USER=
POSTGRES_PASSWORD=

# LangSmith Configuration (optional, for debugging LangChain/LangGraph agents)
# Get your API key from https://smith.langchain.com/
LANGCHAIN_TRACING_V2=           # Enable LangSmith tracing (default: false)
LANGCHAIN_API_KEY=              # Your LangSmith API key
LANGCHAIN_PROJECT=              # Project name in LangSmith (default: raimy-agent)

# Instacart Developer Platform (optional)
# Get API key from https://dashboard.instacart.com/ -> API keys
INSTACART_API_KEY=
INSTACART_API_URL=https://connect.dev.instacart.tools